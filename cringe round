if (sign == 0) {
        return n / 10;
    } else if (sign == 1) {
        n %= MAX;
        if (n % 10 == 5) {
            return (n / 10 + 1) / 2 * 2;
        } else {
            if (n % 10 < 5) return n / 10;
            else return n / 10 + 1;
        }
    } else if (sign == 2) {
        if (f) {
            return n / 10;
        } else {
            return n / 10 + 1;
        }
    } else {
        if (f) { return n / 10 + 1; } else {
            return n / 10;
        }
    }
